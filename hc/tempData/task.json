 },
 "due_date":{
   "type":"timestamp",
   "inpType":"date",
   "placeholder":"What is the Due Date"
 },
 "estEff":{
   "type":"int",
   "inpType":"number",
   "placeholder":"How much time is this going to take?"
 }
}
