{
    "wrkflo":{
        "docId":{
            "required":true,
            "value":"uuid",
            "autogen":true,
            "type":"string"
        },
        "id":{
            "required":true,
            "autogen":true,
            "type":"int"
        },
        "name":{
            "required":true,
            "editable":true,
            "type":"string",
            "inType":"text"

        },
        "lastMod":{
            "required":true,
            "type":"timestamp"
        },
        "type":{
            "required":true,
            "editable":true,
            "type":"string",
            "inType":"text",
            "opts":"wrkfloTypes"
        },
        "sprtType":{
            "required":true,
            "editable":true,
            "type":"string",
            "opts":"wrkfloTypes.{type}.sprtType",
            "inType":"text"
        },
        "descript":{
            "editable":true,
            "type":"string",
            "inType":"textarea"
        },
        "srvType":{
            "type":"string",
            "opts":"wrkfloTypes"
        },
        "model":{
            "editable":true,
            "type":"string",
            "inType":"text"
        },
        "infrmType":{
            "type":"string",
            "opts":"wrkfloTypes.{type}.infrms"
        },
        "outTemp":{
            "type":"string",
            "opts":"_temp-{inInfrmType}",
            "editable":true,
            "type":"string",
            "inType":"text"
        },
        "sprtWrkflos":{
            "editable":true,
            "type":"array",
            "opts":"_wrkflo-{sprtType}",
            "inType":"textarea"
        },
        "wrkfloStatOpts":{
            "type":"array",
            "opts":"sprtWrkflos"
        },
        "owner":{
            "type":"array",
            "opts":"_users-{wrkfloTypes.{type}.owners}"
        },
        "team":{
            "type":"array",
            "opts":"_users-{wrkfloTypes.{type}.team}"
        }
    },
    "wrkfloTypes":{
        "offering":{
            "sprtType":["framework"],
            "infrms":["strategy"],
            "owners":["BD Heads"],
            "team":[""]
        },
        "framework":{
            "sprtType":["process"],
            "infrms":["idea, client, initiative"],
            "owners":[""],
            "team":[""]
        },
        "process":{
            "sprtType":["use_case"],
            "infrms":[""],
            "owners":[""],
            "team":[""]
        },
        "use_case":{
            "sprtType":["funciton"],
            "infrms":[""],
            "owners":[""],
            "team":[""]
        },
        "functions":{
            "sprtType":["deployments"],
            "infrms":[""],
            "owners":[""],
            "team":[""]
        },
        "deployment":{
            "sprtType":["Link to asset"],
            "infrms":[""],
            "owners":[""],
            "team":[""]
        }
    }
}